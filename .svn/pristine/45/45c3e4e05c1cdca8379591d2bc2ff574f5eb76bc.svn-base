<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/digital"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/keyboard_bgd"
    android:baselineAligned="false"
    android:gravity="center"
    android:orientation="horizontal"
    android:paddingBottom="2.0dip"
    android:paddingLeft="3.0dip"
    android:paddingRight="3.0dip"
    android:paddingTop="4.0dip">

    <TableRow android:layout_weight="1.0">

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/keyboard_num_btn_ypadding"
            android:layout_weight="1.0"
            android:orientation="vertical">

            <Button
                android:id="@+id/digital_num_1"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_a_selector"
                android:text="1"
                android:textColor="@color/keyboard_num_color"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />

            <Button
                android:id="@+id/digital_num_4"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_a_selector"
                android:text="4"
                android:textColor="@color/keyboard_num_color"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />

            <Button
                android:id="@+id/digital_num_7"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_a_selector"
                android:text="7"
                android:textColor="@color/keyboard_num_color"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/keyboard_num_btn_ypadding"
            android:layout_weight="1.0"
            android:orientation="vertical">

            <Button
                android:id="@+id/digital_num_2"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_a_selector"
                android:text="2"
                android:textColor="@color/keyboard_num_color"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />

            <Button
                android:id="@+id/digital_num_5"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_a_selector"
                android:text="5"
                android:textColor="@color/keyboard_num_color"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />

            <Button
                android:id="@+id/digital_num_8"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_a_selector"
                android:text="8"
                android:textColor="@color/keyboard_num_color"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/keyboard_num_btn_ypadding"
            android:layout_weight="1.0"
            android:orientation="vertical">

            <Button
                android:id="@+id/digital_num_3"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_a_selector"
                android:text="3"
                android:textColor="@color/keyboard_num_color"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />

            <Button
                android:id="@+id/digital_num_6"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_a_selector"
                android:text="6"
                android:textColor="@color/keyboard_num_color"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />

            <Button
                android:id="@+id/digital_num_9"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_a_selector"
                android:text="9"
                android:textColor="@color/keyboard_num_color"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/keyboard_num_btn_ypadding"
            android:layout_weight="1.0"
            android:orientation="vertical">

            <Button
                android:id="@+id/digital_num_del"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_del_selector"
                android:paddingLeft="1.0dip"
                android:paddingRight="1.0dip" />

            <Button
                android:id="@+id/digital_num_hide"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_change"
                android:paddingLeft="1.0dip"
                android:paddingRight="1.0dip" />

            <Button
                android:id="@+id/digital_num_point"
                android:layout_width="fill_parent"
                android:layout_height="50.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/keyboard_but_b_selector"
                android:gravity="center"
                android:text="."
                android:textColor="@color/white"
                android:textSize="@dimen/keyboard_btn_text_size"
                android:textStyle="bold" />
        </LinearLayout>
    </TableRow>

    <TableRow
        android:layout_weight="1.0"
        android:gravity="center_vertical">

        <Button
            android:id="@+id/digital_num_abc"
            android:layout_width="0.0dip"
            android:layout_height="50.0dip"
            android:layout_weight="1.0"
            android:background="@drawable/keyboard_but_b_selector"
            android:gravity="center"
            android:text="切换"
            android:textColor="@color/white"
            android:textSize="@dimen/keyboard_btn_text_size" />

        <Button
            android:id="@+id/digital_num_0"
            android:layout_width="0.0dip"
            android:layout_height="50.0dip"
            android:layout_weight="1.0"
            android:background="@drawable/keyboard_but_a_selector"
            android:gravity="center"
            android:text="0"
            android:textColor="@color/keyboard_num_color"
            android:textSize="@dimen/keyboard_btn_text_size"
            android:textStyle="bold" />

        <Button
            android:id="@+id/digital_num_00"
            android:layout_width="0.0dip"
            android:layout_height="50.0dip"
            android:layout_weight="1.0"
            android:background="@drawable/keyboard_but_a_selector"
            android:gravity="center"
            android:text="00"
            android:textColor="@color/keyboard_num_color"
            android:textSize="@dimen/keyboard_btn_text_size"
            android:textStyle="bold" />

        <Button
            android:id="@+id/digital_num_enter"
            android:layout_width="0.0dip"
            android:layout_height="50.0dip"
            android:layout_weight="1.0"
            android:background="@drawable/keyboard_enter_selector"
            android:gravity="center" />
    </TableRow>
</TableLayout>