<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
   >
    <include layout="@layout/toolbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/common_padding"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/tv_start"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/textview_corners_bg"
            android:gravity="center"
            android:padding="3dp"
            android:layout_weight="1"

            android:paddingLeft="6dp"
            android:paddingRight="6dp"
            android:text="2012/04/05"
            android:drawableRight="@drawable/drop_down_arrow"

            android:textColor="@color/black"
            android:textSize="?attr/middle" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="-"
            />

        <TextView
            android:id="@+id/tv_end"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/textview_corners_bg"
            android:gravity="center"
            android:padding="3dp"
            android:paddingLeft="6dp"
            android:paddingRight="6dp"
            android:text="2012/04/05"
            android:layout_weight="1"
            android:drawableRight="@drawable/drop_down_arrow"
            android:textColor="@color/black"
            android:textSize="?attr/middle" />


        <Button
            android:id="@+id/bt_commit"
            style="@style/fill_button"
            android:layout_weight="1.3"
            android:layout_height="30dip"
            android:text="查询" />

    </LinearLayout>
    <LinearLayout style="@style/listview_title_ll">
        <TextView
            style="@style/listview_ll_tv_title_left"
            android:text="流水号/类型" />
        <TextView
            style="@style/listview_ll_tv_title_center"
            android:text="操作时间/银行名称" />
        <TextView
            style="@style/listview_ll_tv_title_right"
            android:text="金额/状态" />
    </LinearLayout>
    <ListView
        android:id="@+id/listView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"

        />

</LinearLayout>