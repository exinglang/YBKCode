public class NormalAppKeyPropertyEntity {

	private String tag;//百度推送消息时要绑定的标签
	private boolean showHotNews;//控制是否显示热点消息
	private int showHotNewsInterval;//单位毫秒
	private String communityUrl;//社区网址
	private String rongChatRoomId;//融云聊天室ID
	private String rongChatRoomName;//融云聊天室名称
	private int forceClearHqCache;//强制清除行情缓存，0：不清除；1：清除
	
	public String getRongChatRoomId() {
		return rongChatRoomId;
	}

	public void setRongChatRoomId(String rongChatRoomId) {
		this.rongChatRoomId = rongChatRoomId;
	}

	public String getRongChatRoomName() {
		return rongChatRoomName;
	}

	public void setRongChatRoomName(String rongChatRoomName) {
		this.rongChatRoomName = rongChatRoomName;
	}

	public String getCommunityUrl() {
		return communityUrl;
	}

	public void setCommunityUrl(String communityUrl) {
		this.communityUrl = communityUrl;
	}

	public boolean isShowHotNews() {
		return showHotNews;
	}

	public void setShowHotNews(boolean showHotNews) {
		this.showHotNews = showHotNews;
	}

	public int getShowHotNewsInterval() {
		return showHotNewsInterval;
	}

	public void setShowHotNewsInterval(int showHotNewsInterval) {
		this.showHotNewsInterval = showHotNewsInterval;
	}

	public String getTag() {
		return tag;
	}

	public void setTag(String tag) {
		this.tag = tag;
	}

	public int getForceClearHqCache() {
		return forceClearHqCache;
	}

	public void setForceClearHqCache(int forceClearHqCache) {
		this.forceClearHqCache = forceClearHqCache;
	}

	@Override
	public String toString() {
		return "NormalAppKeyPropertyEntity [tag=" + tag + "]";
	}

}
