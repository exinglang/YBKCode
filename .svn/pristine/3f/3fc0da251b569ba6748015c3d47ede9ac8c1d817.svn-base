<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#212121"
    tools:ignore="HardcodedText">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            >
            <com.puxtech.ybk.hangqing.charts.MiniFenShiView
                android:id="@+id/fenshi_view"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp">
            </com.puxtech.ybk.hangqing.charts.MiniFenShiView>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="8dp"
                            >
                            <TextView
                                style="@style/hangqing_detail_info_name"
                                android:text="委比"
                                />
                            <TextView
                                android:id="@+id/tv_weibi"
                                style="@style/hangqing_detail_info_price"
                                android:text="--"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="8dp"
                            >
                            <TextView
                                style="@style/hangqing_detail_info_name"
                                android:text="现量"
                                />
                            <TextView
                                android:id="@+id/tv_xianliang"
                                style="@style/hangqing_detail_info_price"
                                android:text="--"
                                android:textColor="@color/hangqing_yellow_text"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="8dp"
                            >
                            <TextView
                                style="@style/hangqing_detail_info_name"
                                android:text="内盘"
                                />
                            <TextView
                                android:id="@+id/tv_neipan"
                                style="@style/hangqing_detail_info_price"
                                android:text="--"
                                android:textColor="@color/hangqing_green_text"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="8dp"
                            >
                            <TextView
                                style="@style/hangqing_detail_info_name"
                                android:text="金额"
                                />
                            <TextView
                                android:id="@+id/tv_jine"
                                style="@style/hangqing_detail_info_price"
                                android:text="--"
                                android:textColor="@color/hangqing_yellow_text"
                                />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="8dp"
                            >
                            <TextView
                                style="@style/hangqing_detail_info_name"
                                android:text="委差"
                                />
                            <TextView
                                android:id="@+id/tv_weicha"
                                style="@style/hangqing_detail_info_price"
                                android:text="--"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="8dp"
                            >
                            <TextView
                                style="@style/hangqing_detail_info_name"
                                android:text="量比"
                                />
                            <TextView
                                android:id="@+id/tv_liangbi"
                                style="@style/hangqing_detail_info_price"
                                android:text="--"
                                android:textColor="@color/hangqing_yellow_text"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="8dp"
                            >
                            <TextView
                                style="@style/hangqing_detail_info_name"
                                android:text="外盘"
                                />
                            <TextView
                                android:id="@+id/tv_waipan"
                                style="@style/hangqing_detail_info_price"
                                android:text="--"
                                android:textColor="@color/hangqing_red_text"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="8dp"
                            >
                            <TextView
                                style="@style/hangqing_detail_info_name"
                                android:text="振幅"
                                />
                            <TextView
                                android:id="@+id/tv_zhenfu"
                                style="@style/hangqing_detail_info_price"
                                android:text="--"
                                android:textColor="@color/hangqing_red_text"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>


            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="150dp"
            android:layout_height="match_parent"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    >
                    <LinearLayout
                        android:id="@+id/layout_pankou"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:visibility="visible"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            >
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="卖五"
                                    />
                                <TextView
                                    android:id="@+id/wd_sell_5_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_sell_5_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="卖四"
                                    />
                                <TextView
                                    android:id="@+id/wd_sell_4_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_sell_4_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="卖三"
                                    />
                                <TextView
                                    android:id="@+id/wd_sell_3_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_sell_3_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="卖二"
                                    />
                                <TextView
                                    android:id="@+id/wd_sell_2_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_sell_2_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="卖一" />
                                <TextView
                                    android:id="@+id/wd_sell_1_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_sell_1_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_height="1px"
                            android:layout_width="match_parent"
                            android:layout_marginLeft="8dp"
                            android:layout_marginRight="8dp"
                            android:background="@color/hangqing_detail_wd_name"
                            />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            >
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="买一"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_1_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_1_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="买二"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_2_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_2_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="买三"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_3_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_3_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="买四"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_4_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_4_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>
                            <LinearLayout
                                style="@style/hangqing_detail_wd_row"
                                >
                                <TextView
                                    style="@style/hangqing_detail_wd_name"
                                    android:text="买五"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_5_price"
                                    style="@style/hangqing_detail_wd_price"
                                    android:text="--"
                                    />
                                <TextView
                                    android:id="@+id/wd_buy_5_volume"
                                    style="@style/hangqing_detail_wd_volume"
                                    android:text="--"
                                    />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/layout_mingxi"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:visibility="invisible"
                        android:orientation="vertical"
                        >
                        <ListView
                            android:id="@+id/lv_mingxi"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"></ListView>
                    </LinearLayout>
                </RelativeLayout>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="8dp"
                    >
                    <Button
                        android:id="@+id/btn_pankou"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:minHeight="0dp"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:background="@drawable/hangqing_pankoumingxi_selectedl_bgd"
                        android:text="盘口"
                        android:textSize="12dp"
                        android:textColor="#acc8d3"
                        />
                    <Button
                        android:id="@+id/btn_mingxi"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:minHeight="0dp"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:background="@drawable/hangqing_pan_kou_r_selector"
                        android:text="明细"
                        android:textSize="12dp"
                        android:textColor="#acc8d3"
                        />

                </LinearLayout>

            </LinearLayout>

        </RelativeLayout>


    </LinearLayout>

</LinearLayout>